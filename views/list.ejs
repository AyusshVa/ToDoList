
<%- include("header") -%>
    <div class="box" id="heading">
        <h1>
            <%= heading%>
        </h1>
    </div>
    <div class="box">
        <% for(let i=0; i < newListItems.length; i++) { %>
            <div class="item">
                <input type="checkbox">
            <p>
                <%= newListItems[i] %>
            </p>
            </div>
            <% } %>
            <form class="box" action="/" method="post">
                <input type="text" name="item" placeholder="New item" autocomplete="off">
                <button class="btn" type="submit" name="list" value=<%= heading %>>+</button>
            </form>
            <%- include("footer") -%>